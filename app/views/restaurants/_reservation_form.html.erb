<div class="form-group">
	    <%= f.label :email %>
	    <%= f.text_field :email, class: "form-control", autofocus: true, :required => '' %>
</div>

<div class="form-group">
	    <%= f.label :date %>
	    <%= f.date_field :date, class: "form-control", autofocus: true, :required => '' %>
</div>

<div class="form-group">
	    <%= f.label :time %>
	    <%= f.time_field :time, class: "form-control", autofocus: true, :required => '' %>
</div>

<div class="form-group">
	    <%= f.label :message %>
	    <%= f.text_field :message, class: "form-control", autofocus: true, :required => '' %>
</div>

<div class="form-group">
		<%= f.submit "Submit", class: "btn btn-primary" %>
</div>